{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "jsx": "react-native",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowJs": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@screens/*": [
        "src/screens/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@config": [
        "src/utils/config"
      ],
      "@config/*": [
        "src/utils/config/*"
      ],
      "@content/*": [
        "src/utils/content/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@context": [
        "src/context"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@ui": [
        "src/ui"
      ],
      "@ui/*": [
        "src/ui/*"
      ],
      "@context/*": [
        "src/context/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
